{ // WHEN CHANGING PATHS, REPLACE FRONT SLASHES (\) WITH BACK SLAHES(/)
  "configurations": [
    {
      "name": "windows-gcc-x64",
      "includePath": [
        "${workspaceFolder}/**",
        "C:/msys64/mingw64/include", // REPLACE THIS PATH, WITH THE PATH TO YOUR mingw64/include folder.
        "${workspaceFolder}/include",
        "C:/msys64/ucrt64/include" // REPLACE THIS PATH, WITH THE PATH TO YOUR ucrt/include folder.
      ],
      "defines": [
        "_DEBUG",
        "UNICODE",
        "_UNICODE"
      ],
      "compilerPath": "C:/msys64/ucrt64/bin/gcc.exe", // REPLACE THIS PATH, WITH THE PATH TO YOUR gcc.exe
      "cStandard": "${default}",
      "cppStandard": "${default}",
      "intelliSenseMode": "windows-gcc-x64",
      "browse": {
        "path": [
          "${workspaceFolder}",
          "C:/msys64/mingw64/include", // REPLACE THIS PATH, WITH THE PATH TO YOUR mingw64/include folder, SAME AS LINE 7.
          "${workspaceFolder}/include",
          "C:/msys64/ucrt64/include" // REPLACE THIS PATH, WITH THE PATH TO YOUR ucrt/include folder, SAME AS LINE 9.
          
        ],
        "limitSymbolsToIncludedHeaders": true,
        "databaseFilename": ""
      }
    }
  ],
  "version": 4
}